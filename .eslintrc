
{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/forbid-prop-types": [1, { "forbid": ["any"]}]
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "^16.8.6"
    }
  }
}